{
  "name": "paperbot",
  "version": "1.0.0",
  "description": "A Discord bot",
  "scripts": {
    "start": "ts-node src/Server/index.ts",
    "dev:frontend": "parcel src/Frontend/index.html",
    "build": "cross-env NODE_ENV=production tsc -v && npm run clean && npm run build:typescript && npm run build:frontend",
    "build:frontend": "cross-env NODE_ENV=production parcel build src/Frontend/index.html",
    "build:typescript": "tsc",
    "watch": "nodemon %NODE_DEBUG_OPTION% --exec ts-node src/Server/index.ts",
    "debug": "nodemon --exec node %NODE_DEBUG_OPTION% --inspect -r ts-node/register ./src/Server/index.ts",
    "clean": "rimraf dist",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:fix": "tslint --fix src/**/*.{ts,js}",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register src/tests/*.spec.ts --exit"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "eris": "^0.9.0",
    "express": "^4.16.4",
    "node-fetch": "^2.3.0",
    "apollo-server-express": "^2.3.1",
    "graphql": "^14.0.2",
    "reflect-metadata": "^0.1.12",
    "sqlite3": "^4.0.4",
    "typeorm": "^0.2.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-pose": "^4.0.5",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "styled-components": "^4.1.3",
    "lodash.merge": "^4.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/dotenv": "^6.1.0",
    "@types/graphql": "^14.0.4",
    "@types/node": "^10.12.18",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "parcel-bundler": "^1.11.0",
    "rimraf": "^2.6.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paperfeed/paperbot.git"
  },
  "keywords": [
    "discord",
    "nodejs",
    "express",
    "eris"
  ],
  "author": "Aldert Vaandering",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Paperfeed/paperbot/issues"
  },
  "homepage": "https://github.com/Paperfeed/paperbot#readme"
}
